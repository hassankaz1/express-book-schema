{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "author",
        "language",
        "pages",
        "title",
        "year"
    ],
    "properties": {
        "author": {
            "type": "string",
            "default": "",
            "title": "The author Schema",
            "examples": [
                "Marijn Haverbeke"
            ]
        },
        "language": {
            "type": "string",
            "default": "",
            "title": "The language Schema",
            "examples": [
                "english"
            ]
        },
        "pages": {
            "type": "integer",
            "default": 0,
            "title": "The pages Schema",
            "examples": [
                472
            ]
        },
        "title": {
            "type": "string",
            "default": "",
            "title": "The title Schema",
            "examples": [
                "Eloquent JavaScript, Third Edition"
            ]
        },
        "year": {
            "type": "integer",
            "default": 0,
            "title": "The year Schema",
            "examples": [
                2018
            ]
        }
    },
    "examples": [
        {
            "author": "Marijn Haverbeke",
            "language": "english",
            "pages": 472,
            "title": "Eloquent JavaScript, Third Edition",
            "year": 2018
        }
    ]
}